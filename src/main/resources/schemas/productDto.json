{
  "type": "object",
  "$schema": "http://json-schema.org/draft-03/schema",
  "title": "ProductDto",
  "description": "A single product",
  "properties": {
    "id": {
      "type": "string",
      "title": "id"
    },
    "productId": {
      "type": "string",
      "title": "productId",
      "required": true
    },
    "productName": {
      "type": "string",
      "title": "productName",
      "required": true
    },
    "productCode": {
      "type": "string",
      "title": "productCode"
    },
    "productPackSize": {
      "type": "number",
      "title": "productPackSize",
      "required": true
    },
    "productCategoryId": {
      "type": "string",
      "title": "productCategoryId",
      "required": true
    },
    "productCategoryDisplayName": {
      "type": "string",
      "title": "productCategoryDisplayName",
      "required": true
    },
    "productCategoryDisplayOrder": {
      "type": "integer",
      "title": "productCategoryDisplayOrder",
      "required": true
    },
    "dosesPerMonth": {
      "type": "integer",
      "title": "dosesPerMonth",
      "required": true
    },
    "active": {
      "type": "boolean",
      "title": "active",
      "required": true
    },
    "fullSupply": {
      "type": "boolean",
      "title": "fullSupply",
      "required": true
    },
    "displayOrder": {
      "type": "integer",
      "title": "displayOrder"
    },
    "maxMonthsOfStock": {
      "type": "integer",
      "title": "maxMonthsOfStock"
    },
    "pricePerPack": {
      "type": "number",
      "title": "pricePerPack"
    }
  }
}
