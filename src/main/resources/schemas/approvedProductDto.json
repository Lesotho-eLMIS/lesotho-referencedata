{
  "type": "object",
  "$schema": "http://json-schema.org/draft-03/schema",
  "title": "ApprovedProductDto",
  "description": "A single approved product",
  "properties": {
    "id": {
      "type": "string",
      "title": "id",
      "required": true
    },
    "product": {
      "type": "object",
      "$ref": "productDto.json",
      "title": "product",
      "required": true
    },
    "maxMonthsOfStock": {
      "type": "number",
      "title": "maxMonthsOfStock",
      "required": true
    },
    "minMonthsOfStock": {
      "type": "number",
      "title": "minMonthsOfStock"
    },
    "emergencyOrderPoint": {
      "type": "number",
      "title": "emergencyOrderPoint"
    }
  }
}
