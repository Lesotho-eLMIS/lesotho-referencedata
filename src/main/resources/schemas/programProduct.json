{
  "type": "object",
  "$schema": "http://json-schema.org/draft-03/schema",
  "title": "ProgramProduct",
  "description": "A single programProduct",
  "properties": {
    "id": {
      "type": "string",
      "title": "id",
      "required": true
    },
    "program": {
      "type": "object",
      "$ref": "program.json",
      "title": "program",
      "required": true
    },
    "product": {
      "type": "object",
      "$ref": "orderableProduct.json",
      "title": "product",
      "required": true
    },
    "dosesPerMonth": {
      "type": "integer",
      "title": "dosesPerMonth",
      "required": true
    },
    "active": {
      "type": "boolean",
      "title": "active",
      "required": true
    },
    "productCategory": {
      "type": "object",
      "$ref": "productCategory.json",
      "title": "productCategory",
      "required": true
    },
    "fullSupply": {
      "type": "boolean",
      "title": "fullSupply",
      "required": true
    },
    "displayOrder": {
      "type": "integer",
      "title": "displayOrder"
    },
    "maxMonthsStock": {
      "type": "integer",
      "title": "maxMonthsStock"
    },
    "pricePerPack": {
      "type": "number",
      "title": "pricePerPack"
    }
  }
}