{
  "type": "object",
  "$schema": "http://json-schema.org/draft-03/schema",
  "title": "GeographicZone",
  "description": "A single geographicZone",
  "properties": {
    "id": {
      "type": "string",
      "title": "id"
    },
    "code": {
      "type": "string",
      "title": "code",
      "required": true
    },
    "name": {
      "type": "string",
      "title": "name"
    },
    "level": {
      "type": "object",
      "$ref": "geographicLevel.json",
      "required": true
    },
    "catchmentPopulation": {
      "type": "integer",
      "title": "catchmentPopulation"
    },
    "latitude": {
      "type": "number",
      "title": "latitude"
    },
    "longitude": {
      "type": "number",
      "title": "longitude"
    }
  }
}
