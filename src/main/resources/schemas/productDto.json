{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "ProductDto",
  "description": "A single product",
  "properties": {
    "id": {
      "type": "string",
      "title": "id"
    },
    "productId": {
      "type": "string",
      "title": "productId"
    },
    "productName": {
      "type": "string",
      "title": "productName"
    },
    "productCategoryId": {
      "type": "string",
      "title": "productCategoryId"
    },
    "productCategoryDisplayName": {
      "type": "string",
      "title": "productCategoryDisplayName"
    },
    "productCategoryDisplayOrder": {
      "type": "integer",
      "title": "productCategoryDisplayOrder"
    },
    "dosesPerMonth": {
      "type": "integer",
      "title": "dosesPerMonth"
    },
    "active": {
      "type": "boolean",
      "title": "active"
    },
    "fullSupply": {
      "type": "boolean",
      "title": "fullSupply"
    },
    "displayOrder": {
      "type": "integer",
      "title": "displayOrder"
    },
    "maxMonthsStock": {
      "type": "integer",
      "title": "maxMonthsStock"
    },
    "pricePerPack": {
      "type": "number",
      "title": "pricePerPack"
    }
  },
  "required": ["productId", "productName", "productCategoryId", "productCategoryDisplayName",
    "productCategoryDisplayOrder", "dosesPerMonth", "active", "fullSupply"]
}