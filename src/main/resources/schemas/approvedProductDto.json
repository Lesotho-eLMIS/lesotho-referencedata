{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "ApprovedProductDto",
  "description": "A single approved product",
  "properties": {
    "id": {
      "type": "string",
      "title": "id"
    },
    "programOrderable": {
      "type": "object",
      "$ref": "programOrderableDto.json",
      "title": "programOrderable"
    },
    "maxStock": {
      "type": "number",
      "title": "maxStock"
    },
    "minStock": {
      "type": "number",
      "title": "minStock"
    },
    "emergencyOrderPoint": {
      "type": "number",
      "title": "emergencyOrderPoint"
    }
  },
  "required": ["id", "programOrderable", "maxStock"]
}
