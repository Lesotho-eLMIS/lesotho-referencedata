{
  "type": "object",
  "$schema": "http://json-schema.org/draft-03/schema",
  "title": "Facility",
  "description": "A single facility",
  "properties": {
    "id": {
      "type": "string",
      "title": "id"
    },
    "code": {
      "type": "string",
      "title": "code",
      "required": true
    },
    "name": {
      "type": "string",
      "title": "name"
    },
    "description": {
      "type": "string",
      "title": "description"
    },
    "geographicZone": {
      "type": "object",
      "$ref": "geographicZone.json",
      "required": true
    },
    "type": {
      "type": "object",
      "$ref": "facilityType.json",
      "required": true
    },
    "operator": {
      "type": "object",
      "$ref": "facilityOperator.json"
    },
    "active": {
      "type": "boolean",
      "title": "active",
      "required": true
    },
    "goLiveDate": {
      "type": "string",
      "title": "goLiveDate"
    },
    "goDownDate": {
      "type": "string",
      "title": "goDownDate"
    },
    "comment": {
      "type": "string",
      "title": "comment"
    },
    "enabled": {
      "type": "boolean",
      "title": "enabled",
      "required": true
    },
    "openLmisAccessible": {
      "type": "boolean",
      "title": "openLmisAccessible"
    },
    "supportedPrograms": {
      "type": "array",
      "title": "supportedPrograms",
      "items": {
        "type": "object",
        "$ref": "supportedProgram.json"
      },
      "uniqueItems": false
    }
  }
}
