{
  "type": "object",
  "$schema": "http://json-schema.org/draft-03/schema",
  "title": "Supported Program",
  "description": "A supported program",
  "properties": {
    "id": {
      "type": "string",
      "title": "id"
    },
    "code": {
      "type": "string",
      "title": "code",
      "required": true
    },
    "name": {
      "type": "string",
      "title": "name"
    },
    "description": {
      "type": "string",
      "title": "description"
    },
    "programActive": {
      "type": "boolean",
      "title": "programActive",
      "required": true
    },
    "periodsSkippable": {
      "type": "boolean",
      "title": "periodsSkippable",
      "required": true
    },
    "showNonFullSupplyTab": {
      "type": "boolean",
      "title": "showNonFullSupplyTab",
      "required": true
    },
    "supportActive": {
      "type": "boolean",
      "title": "supportActive",
      "required": true
    },
    "supportStartDate": {
      "type": "string",
      "title": "supportStartDate"
    }
  }
}