{
  "type": "object",
  "$schema": "http://json-schema.org/draft-03/schema",
  "title": "Orderable Product",
  "description": "A single orderableProduct",
  "properties": {
    "id": {
      "type": "string",
      "title": "id",
      "required": true
    },
    "productCode": {
      "type": "string",
      "title": "productCode",
      "required": true
    },
    "name": {
      "type": "string",
      "title": "name"
    },
    "packSize": {
      "type": "number",
      "title": "packSize"
    },
    "packRoundingThreshold": {
      "type": "integer",
      "title": "packRoundingThreshold",
      "required": true
    },
    "roundToZero": {
      "type": "boolean",
      "title": "roundToZero",
      "required": true
    },
    "programs": {
      "type": "array",
      "title": "programs",
      "items": {
        "type": "object",
        "$ref": "programProduct.json"
      },
      "uniqueItems": true
    }
  }
}