{
  "type": "object",
  "$schema": "http://json-schema.org/draft-03/schema",
  "title": "Program",
  "description": "A single program",
  "properties": {
    "id": {
      "type": "string",
      "title": "id",
      "required": true
    },
    "code": {
      "type": "string",
      "title": "code",
      "required": true
    },
    "name": {
      "type": "string",
      "title": "name"
    },
    "description": {
      "type": "string",
      "title": "description"
    },
    "active": {
      "type": "boolean",
      "title": "active"
    },
    "periodsSkippable": {
      "type": "boolean",
      "title": "periodsSkippable",
      "required": true
    },
    "showNonFullSupplyTab": {
      "type": "boolean",
      "title": "showNonFullSupplyTab"
    }
  }
}